<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.wingshield.technologies.demopostdata">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name=".utils.AppController"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.DemoPostData">
        <activity android:name=".presentation.ui.activities.impl.IntroSliderActivity"/>
        <activity android:name=".presentation.ui.activities.impl.OtpVerifyActivity" />
        <activity android:name=".presentation.ui.activities.impl.LoginRegisterActivity" />
        <activity
            android:name=".presentation.ui.activities.impl.SplashActivity"
            android:theme="@style/FullScreen" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".presentation.ui.activities.impl.BookingConfirmActivity" />
        <activity android:name=".presentation.ui.activities.impl.HistoryActivity" />
        <activity android:name=".presentation.ui.activities.impl.AppointmentActivity" />
        <activity android:name=".presentation.ui.activities.impl.UserActivity">

        </activity>
        <activity android:name=".presentation.ui.activities.impl.MainActivity" />
    </application>

</manifest>