<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">

    <item>
        <shape android:shape="rectangle">
            <solid android:color="#FFA471FF" />
            <corners android:bottomRightRadius="60dp" />
        </shape>
    </item>
    <item android:bottom="200dp"  android:right="100dp" android:top="200dp">
        <shape android:shape="oval">
            <solid android:color="#33FFFFFF" />
        </shape>
    </item>
    <item android:bottom="300dp"  android:left="100dp" android:top="100dp">
        <shape android:shape="oval">
            <solid android:color="#337B30FF" />
        </shape>
    </item>
    <item android:top="300dp"  android:left="100dp" android:bottom="100dp">
        <shape android:shape="oval">
            <solid android:color="#33CACACA" />
        </shape>
    </item>
</layer-list>